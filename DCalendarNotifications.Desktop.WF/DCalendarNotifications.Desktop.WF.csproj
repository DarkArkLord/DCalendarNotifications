<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>disable</ImplicitUsings>
    <ApplicationIcon>calendar.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="calendar.ico" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="calendar.ico" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\DCalendarNotifications.Core\DCalendarNotifications.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="calendar.ico" />
  </ItemGroup>

</Project>